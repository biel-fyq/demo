.barrier-top {
  width: 100%;
  padding: 0 160rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: center;
}

.top-content {
  text-align: center;
  margin: 26rpx 0 68rpx;
  position: relative;
  display: flex;
  flex: 1;
  flex-direction: column;
  /* 垂直排列：图片在上，文字在下 */
  align-items: center;
  /* 水平居中 */
}

.title {
  font-family: ZCOOL KuaiLe;
  font-weight: 400;
  font-style: Regular;
  font-size: 36rpx;
  line-height: 140%;
  letter-spacing: 0.6rpx;
  color: #000000;
  position: relative;
  z-index: 2;
}

.decorate-img {
  display: none;
  width: 180rpx;
  height: 60rpx;
  top: 30rpx;
  z-index: 1;
  position: absolute;
}

.barrier-content,
.wall-content {
  position: relative;
  height: fit-content;
}

.barrier-content-bag-container {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
  position: relative;
}

.barrier-content-bag-title {
  position: absolute;
  top: 20rpx;
  color: #FFFFFF;
  font-family: ZCOOL KuaiLe;
  font-weight: 400;
  font-style: Regular;
  font-size: 40rpx;
  line-height: 140%;
  letter-spacing: 0.6rpx;
}

.barrier-content-bag {
  width: 456rpx;
  height: 738rpx;
  max-width: 90vw;
  max-height: 60vh;
}

.list-center-overlay {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  z-index: 10;
  margin-top: -33.866666666%;
  margin-bottom: 32rpx;
}

.tag-item {
  display: flex;
  justify-content: center;
  align-items: center;
}

.tag-text {
  font-family: ZCOOL KuaiLe;
  font-weight: 400;
  font-style: Regular;
  font-size: 40rpx;
  line-height: 140%;
  letter-spacing: 0.6rpx;
  color: #000000;
  white-space: nowrap;
}

/* .small-tag, */
.tag-item {
  margin-left: 59rpx;
  width: 284rpx;
  height: 114rpx;
  z-index: 2;
  background-size: 100% 100%;
  background-repeat: no-repeat;
  background-position: center;
  background-image: url("data:image/png;base64,");
}

.tag-item:nth-child(2) {
  margin-left: 6rpx;
  width: 314rpx;
  height: 118rpx;
  box-sizing: border-box;
  background-image: url("data:image/png;base64,");
}

.tag-item:nth-child(3) {
  margin-left: 88rpx;
  margin-top: -16rpx;
  width: 562rpx;
  height: 142rpx;
  z-index: 1;
  background-image: url("data:image/png;base64,");
}

.tag-item:nth-child(4) {
  margin-left: 90rpx;
  margin-top: -40rpx;
  width: 562rpx;
  height: 142rpx;
  background-image: url("data:image/png;base64,");
}

.tag-item:nth-child(5) {
  margin-left: 90rpx;
  margin-top: -38rpx;
  height: 142rpx;
  width: 560rpx;
  background-image: url("data:image/png;base64,");
}

.clear-button {
  padding: 0;
  height: 171rpx;
  background: initial;
  background-repeat: no-repeat;
  background-size: 100% 100%;
  background-image: url("data:image/png;base64,");
}

.wall-content .clear-button {
  position: absolute;
  height: 184rpx;
  top: 962rpx;
  background-image: url("data:image/png;base64,");
}

.bg-wall {
  width: 750rpx;
  position: absolute;
  top: -266rpx;
  z-index: -1;
}

.wall-content .text-content {
  width: 458rpx;
  margin: 0 auto;
  padding-top: 260rpx;
}

.wall-content .text-content text {
  font-family: ZCOOL KuaiLe;
  font-weight: 400;
  font-style: Regular;
  font-size: 40rpx;
  line-height: 140%;
  letter-spacing: 0.6rpx;
}

.container {
  display: flex;
  flex-direction: column;
}

.content-container {
  overflow: hidden;
  position: relative;
  display: contents;
}

.content-wrapper {
  display: flex;
  height: 100%;
  width: 100vw;
}

.content-item {
  width: 100%;
  flex-shrink: 0;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.animated-content {
  width: 100%;
  opacity: 0; /* 初始状态透明 */
}

.tab-active .decorate-img {
  display: block;
}


/* 小屏幕适配 (iPhone 8 及以下) */
@media screen and (max-width: 375px) {
  .small-tag-icon {
    width: 200rpx;
    height: 80rpx;
  }
  
  .large-tag-icon {
    width: 350rpx;
    height: 90rpx;
  }
  
  .small-tag-text,
  .large-tag-text {
    font-size: 18rpx;
  }
  
  .barrier-content-bag {
    max-width: 85vw;
    max-height: 50vh;
    z-index: -1;
  }
}

/* 超小屏幕适配 (iPhone SE 等) */
@media screen and (max-width: 320px) {
  .small-tag-icon {
    width: 160rpx;
    height: 65rpx;
  }
  
  .large-tag-icon {
    width: 280rpx;
    height: 75rpx;
  }
  
  .small-tag-text,
  .large-tag-text {
    font-size: 16rpx;
  }
  
  .barrier-content-bag {
    max-width: 90vw;
    max-height: 45vh;
  }
}

/* 大屏幕适配 (iPhone Plus 及以上) */
@media screen and (min-width: 414px) {
  .small-tag-icon {
    width: 320rpx;
    height: 130rpx;
  }
  
  .large-tag-icon {
    width: 600rpx;
    height: 150rpx;
  }
  
  .small-tag-text,
  .large-tag-text {
    font-size: 26rpx;
  }
}