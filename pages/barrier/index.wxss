.barrier-top {
  padding: 0 160rpx;
  display: flex;
  margin: 0 auto;
  /* 如需整体居中可加： */
  justify-content: center;
}

.top-content {
  text-align: center;
  margin: 26rpx 0 68rpx;
  position: relative;
  display: flex;
  flex: 1;
  flex-direction: column;
  /* 垂直排列：图片在上，文字在下 */
  align-items: center;
  /* 水平居中 */
}

.title {
  font-family: ZCOOL KuaiLe;
  font-weight: 400;
  font-style: Regular;
  font-size: 36rpx;
  line-height: 140%;
  letter-spacing: 0.6rpx;
  color: #000000;
  position: relative;
  z-index: 2;
}

.decorate-img {
  width: 180rpx;
  top: 30rpx;
  z-index: 1;
  position: absolute;
}

.barrier-content {
  position: relative;
}

.barrier-content-bag-container {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
  position: relative;
}

.barrier-content-bag-title {
  position: absolute;
  top: 20rpx;
  color: #FFFFFF;
  font-family: ZCOOL KuaiLe;
  font-weight: 400;
  font-style: Regular;
  font-size: 40rpx;
  line-height: 140%;
  letter-spacing: 0.6rpx;

}

.barrier-content-bag {
  width: 456rpx;
  height: 738rpx;
  max-width: 90vw;
  max-height: 60vh;
}

.tags-row {
  display: flex;
  z-index: 1;
}

.small-tag {
  position: relative;
  display: flex;
  justify-content: center;
  align-items: center;
}

.small-tag-icon {
  position: absolute;
  width: 282rpx;
  height: 114rpx;
}

.small-tag-text {
  position: absolute;
  color: #000000;
  font-family: ZCOOL KuaiLe;
  font-weight: 400;
  font-style: Regular;
  font-size: 20rpx;
  line-height: 140%;
  letter-spacing: 0.6rpx;
  text-align: center;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  white-space: nowrap;
}

.large-tag-icon {
  width: 562rpx;
  height: 142rpx;
}
.large-tag {
  position: relative;
  display: flex;
  justify-content: center;
  align-items: center;
}

.large-tag-text {
  position: absolute;
  color: #000000;
  font-family: ZCOOL KuaiLe;
  font-weight: 400;
  font-style: Regular;
  font-size: 20rpx;
  line-height: 140%;
  letter-spacing: 0.6rpx;
  text-align: center;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  white-space: nowrap;
}

.list-center-overlay {
  display: flex;
  flex-direction: column;
  z-index: 10;
  margin-top: -33.866666666%;
  margin-bottom: 32rpx;
}

.small-tag {
  margin-left: 59rpx;
  width: 284rpx;
  height: 114rpx;
  background-size: 100% 100%;
  background-repeat: no-repeat;
  background-position: center;
  background-image: url("data:image/png;base64,");
}

.small-tag:nth-child(2) {
  background-image: url("data:image/png;base64,");
  width: 314rpx;
  height: 118rpx;
  box-sizing: border-box;
}

.small-tag + .small-tag {
  margin-left: 6rpx;
}

.large-tag {
  margin-left: 88rpx;
  margin-top: -16rpx;
  width: 562rpx;
  height: 142rpx;
  background-size: 100% 100%;
  background-repeat: no-repeat;
  background-position: center;
  background-image: url("data:image/png;base64,");
}

.large-tag:nth-child(2) {
  margin-left: 90rpx;
  margin-top: -40rpx;
  background-image: url("data:image/png;base64,");
}

.large-tag:nth-child(3) {
  margin-left: 90rpx;
  margin-top: -38rpx;
  width: 560rpx;
  background-image: url("data:image/png;base64,");
}

.clear-button {
  padding: 0;
  height: 171rpx;
  background: initial;
  background-repeat: no-repeat;
  background-size: 100% 100%;
  background-image: url("data:image/png;base64,");
}

/* 小屏幕适配 (iPhone 8 及以下) */
@media screen and (max-width: 375px) {
  .small-tag-icon {
    width: 200rpx;
    height: 80rpx;
  }
  
  .large-tag-icon {
    width: 350rpx;
    height: 90rpx;
  }
  
  .small-tag-text,
  .large-tag-text {
    font-size: 18rpx;
  }
  
  .barrier-content-bag {
    max-width: 85vw;
    max-height: 50vh;
  }
}

/* 超小屏幕适配 (iPhone SE 等) */
@media screen and (max-width: 320px) {
  .small-tag-icon {
    width: 160rpx;
    height: 65rpx;
  }
  
  .large-tag-icon {
    width: 280rpx;
    height: 75rpx;
  }
  
  .small-tag-text,
  .large-tag-text {
    font-size: 16rpx;
  }
  
  .barrier-content-bag {
    max-width: 90vw;
    max-height: 45vh;
  }
}

/* 大屏幕适配 (iPhone Plus 及以上) */
@media screen and (min-width: 414px) {
  .small-tag-icon {
    width: 320rpx;
    height: 130rpx;
  }
  
  .large-tag-icon {
    width: 600rpx;
    height: 150rpx;
  }
  
  .small-tag-text,
  .large-tag-text {
    font-size: 26rpx;
  }
}